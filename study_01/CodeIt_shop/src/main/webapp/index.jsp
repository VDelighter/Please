<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>SimpleShop</title>
<link rel="stylesheet" href="css/styles.css">
</head>
<body>
	<div class="navbar">
		<a href="#" id="logo">
			<img alt="logo" src="images/logo.png" width="149">
		</a>
		<ul id="menu">
			<li><a href="#">Contact</a></li>
			<li><a href="#">Shop</a></li>
			<li><a href="#">Cart</a></li>
			<li><a href="#">Login</a></li>
		</ul>
	</div>
	<div class="hero-header"></div>
	<div class="products">
	
		<h3>Our New Products</h3>
		
		<div class="product-list">
			<a href="#" class="product">
				<img alt="pro" src="images/sunglasses.jpg" width="225">
				<div class="product-name">Sunglasses</div>
				<div class="product-price">49,000</div>
			</a>
			<a href="#" class="product">
				<img alt="pro" src="images/tassel_loafer.jpg" width="225">
